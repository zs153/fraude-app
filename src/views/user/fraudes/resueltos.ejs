<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="utf-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1, viewport-fit=cover" />
  <meta http-equiv="X-UA-Compatible" content="ie=edge" />
  <title>Fraudes</title>
  <link rel="icon" href="/img/favicon.ico" type="image/x-icon" />
  <!-- styles -->
  <link rel="stylesheet" href="/css/styles.css">
</head>

<body class="antialiased">
  <div class="wrapper">
    <%- include('../../partials/header') %>
      <%- include('../../partials/navbar', {opcion: 'FRAUDE' }) %>
        <div class="page-wrapper">
          <div class="page-body">
            <div class="container-xl">
              <div class="row">
                <div class="col-12">
                  <div class="card">
                    <div class="card-header">
                      <h3 class="card-title">Fraudes Resueltos</h3>
                      <div class="col-auto ms-auto">
                        <div class="d-flex">
                          <form action="/user/fraudes/resueltos" method="GET">
                            <div class="input-icon me-3">
                              <span class="input-icon-addon">
                                <svg xmlns="http://www.w3.org/2000/svg" class="icon" width="24" height="24"
                                  viewBox="0 0 24 24" stroke-width="2" stroke="currentColor" fill="none"
                                  stroke-linecap="round" stroke-linejoin="round">
                                  <path stroke="none" d="M0 0h24v24H0z" fill="none"></path>
                                  <circle cx="10" cy="10" r="7"></circle>
                                  <line x1="21" y1="21" x2="15" y2="15"></line>
                                </svg>
                              </span>
                              <input type="text" class="form-control d-inline-block me-3" id="buscarFraudeBox"
                                name="part" placeholder="Buscar..." aria-label="Buscar en fraudes">
                            </div>
                          </form>
                          <a href="/user/fraudes" class="btn">
                            <svg xmlns="http://www.w3.org/2000/svg" class="icon" height="20" width="20"
                              viewBox="0 0 20 20">
                              <path
                                d="M10 12.792q1.167 0 1.979-.813.813-.812.813-1.979t-.813-1.979q-.812-.813-1.979-.813t-1.979.813q-.813.812-.813 1.979t.813 1.979q.812.813 1.979.813ZM10 12q-.833 0-1.417-.583Q8 10.833 8 10q0-.833.583-1.417Q9.167 8 10 8q.833 0 1.417.583Q12 9.167 12 10q0 .833-.583 1.417Q10.833 12 10 12Zm0 3.167q-2.625 0-4.802-1.407Q3.021 12.354 1.896 10q1.125-2.354 3.292-3.76Q7.354 4.833 10 4.833q2.625 0 4.802 1.407 2.177 1.406 3.302 3.76-1.125 2.354-3.292 3.76-2.166 1.407-4.812 1.407ZM10 10Zm0 4.5q2.333 0 4.312-1.208 1.98-1.209 3.042-3.292-1.062-2.083-3.042-3.292Q12.333 5.5 10 5.5T5.688 6.708Q3.708 7.917 2.646 10q1.062 2.083 3.042 3.292Q7.667 14.5 10 14.5Z" />
                            </svg>
                            Pendientes
                          </a>
                        </div>
                      </div>
                    </div>
                    <div class="card-body">
                      <table class="table table-vcenter table-striped sortable" id="tblFraudes">
                        <thead>
                          <tr class="header">
                            <th class="w-4"></th>
                            <th class="w-8">Oficina</th>
                            <th class="w-6">Fecha</th>
                            <th class="w-5">Ejercicio</th>
                            <th class="w-8">NIF</th>
                            <th>Nombre</th>
                            <th class="w-15">Tipo</th>
                            <th>Observaciones</th>
                            <th class="w-5"></th>
                          </tr>
                        </thead>
                        <tbody id="table-body">
                        </tbody>
                      </table>
                      <div id="pagination-wrapper" class="py-1"></div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
        <%- include('../../partials/footer') %>
  </div>

  <!-- modales -->
  <div class="modal modal-blur fade" id="modal-desasignar" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="modal-dialog modal-sm modal-dialog-centered" role="document">
      <div class="modal-content">
        <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
        <div class="modal-status bg-danger"></div>
        <div class="modal-body text-center py-4">
          <svg xmlns="http://www.w3.org/2000/svg" class="icon mb-2 text-danger icon-lg" width="24" height="24"
            viewBox="0 0 24 24" stroke-width="2" stroke="currentColor" fill="none" stroke-linecap="round"
            stroke-linejoin="round">
            <path stroke="none" d="M0 0h24v24H0z" fill="none"></path>
            <path d="M12 9v2m0 4v.01"></path>
            <path d="M5 19h14a2 2 0 0 0 1.84 -2.75l-7.1 -12.25a2 2 0 0 0 -3.5 0l-7.1 12.25a2 2 0 0 0 1.75 2.75"></path>
          </svg>
          <h3>Desasignar fraude resuelto</h3>
          <div id="msgdes"></div>
        </div>
        <div class="modal-footer">
          <div class="w-100">
            <div class="row">
              <div class="col">
                <a href="" class="btn btn-white w-100" data-bs-dismiss="modal">Cancelar</a>
              </div>
              <div class="col">
                <form action="/admin/fraudes/desasignar" method="POST">
                  <input type="hidden" id="iddesa" name="idfrau" value="">
                  <button type="submit" class="btn btn-danger w-100">Desasignar fraude</button>
                </form>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>

  <script>
    const orgList = <%- JSON.stringify(datos.fraudes) %>
    const estadosFraude = <%- JSON.stringify(datos.estadosFraude) %>
    const cursor = <%- JSON.stringify(datos.cursor) %>
    const hasNextFras = <%- datos.hasNextFras %>
    const hasPrevFras = <%- datos.hasPrevFras %>
  </script>
  <script src="/js/ayuda.min.js"></script>
  <script src="/js/indexResueltos.min.js"></script>
</body>

</html>